<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="E:\maven_repos\mysql\mysql-connector-java\5.1.28\mysql-connector-java-5.1.28.jar" />
	<context id="Mysql2Tables" targetRuntime="MyBatis3">
		
		<!--数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.18.6.91:3306/credit_test"
			userId="root" password="root">
		</jdbcConnection>
		
		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		
		 <!-- 生成实体类 指定包名 -->
		<javaModelGenerator targetPackage="com.test.model" targetProject="src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<!--生成SQLMAP文件 -->
		<sqlMapGenerator targetPackage="com.test.mapper"  targetProject="src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.test.mapper"  targetProject="src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<table schema="user" tableName="user" domainObjectName="user" 
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false" 
			selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>

